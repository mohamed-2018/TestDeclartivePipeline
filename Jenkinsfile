pipeline
{
  agent any
  stages 
  {
    stage('SCM CheckOut')
    {
      steps
      {
        echo 'gitting from github'
        echo 'SCM CHECKOUT COMPLETED'
        
      }
    }
     stage('build')
    {
      steps
      {
        echo 'Build COMPLETED'
        
      }
    }
     stage('Test')
    {
      steps
      {
        echo 'Test COMPLETED'
        
      }
    }
     stage('Deploy')
    {
      steps
      {
        echo 'Deploy COMPLETED'
        
      }
    }
  }
}
